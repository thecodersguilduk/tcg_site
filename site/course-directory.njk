---
layout: default
meta_title: Web Development & Digital Skills Course Directory
meta_description: See All Our Digital Skills Courses And Web Development Apprenticeships Available, including Remote Workshops & Coding Bootcamps.
---


<main id="main" class="main flex flex-1 flex-col focus:outline-none" tabindex="-1">
   <section class="bg-white section-rounded pt-48 md:pt-64 pb-24">
      <div class="inner">
         <h2 class="my-8">{{meta_title}}</h2>
         <div class="py-6">
            <span class="text-blue-200 font-medium mr-2 md:mr-6">Jump to:</span> <a class="font-bold mr-2 md:mr-6 no-border" href="#courses">Courses</a> <a class="font-bold mr-2 md:mr-6 no-border" href="#apprenticeships">Apprenticeships</a>
         </div>
         <div class="main-content flex flex-wrap mt-12">
            <div class="filters-sidebar w-full md:w-3/12 px-6 mb-12 md:mb-0">
               <div class="flex justify-between border-b-1 pb-4 mb-4">
                  <h3 class="text-blue-200 mb-0">Filters</h3>
                  <button class="mb-0 text-xs reset-filters cursor-pointer">Reset Filters</span>
               </div>
               <div>
                  <form class="cdFiltersForm">
                     <div class="my-4">
                        <p class="heading"><strong>Topics</strong></p>
                        <select name="topics" id="topics" class="w-full">
                           <option value=null selected>Select a topic</option>
                           {% for item in topics %}
                              <option value="{{ item.name }}">{{ item.name }}</option>
                           {% endfor %}
                        </select>
                     </div>
                     {# <div class="my-4">
                        <p class="heading">Location</p>
                        <select name="locations" class="w-full">
                           <option>Remote</option>
                           <option>Classroom</option>
                        </select>
                     </div> #}
                     <div class="my-4">
                        <p class="heading">Course Type</label>
                        {% for item in courseTypes %}
                           {% if item.courseType !== 'Apprenticeships' %}
                              <div class="flex align-center my-2">
                                 <input name="courseType" id="{{ item.courseType }}" type="checkbox" value="{{ item.courseType }}" />
                                 <label for="{{ item.courseType }}">{{ item.courseType | truncate(25) }}</label>
                              </div>
                           {% endif %}
                        {% endfor %}
                     </div>
                     <div class="my-4">
                        <p class="heading">Attendance</p>
                        <div class="flex justify-between align-center my-2">
                           <input name="attendance" type="checkbox" value="full time" />
                           <label>Full Time</label>
                        </div>
                        <div class="flex justify-between align-center my-2">
                           <input name="attendance" type="checkbox" value="part time" />
                           <label>Part Time</label>
                        </div>
                        <div class="flex justify-between align-center my-2">
                           <input name="attendance" type="checkbox" value="on demand" />
                           <label>On Demand</label>
                        </div>
                     </div>
                     <div class="my-4">
                        <p class="heading">Course Length</p>
                        {% for duration in courseDurations %}
                           <div class="flex justify-between align-center my-2">
                              <input name="length" type="checkbox" value="{{duration}}" class="fancy-check" />
                              <label>{{duration}}</label>
                           </div>
                        {% endfor %}
                     </div>
                  </form>
               </div>
            </div>
            <div class="w-full md:w-9/12  mx-auto lg:max-w-none md:pl-6 parent">
               <div class="flex justify-between border-b-1 pb-4 mb-4">
                  <h3 class="text-blue-200 mb-0"><span class="numberCourses"></span> Courses Found</h3>
               </div>
               {% for type in courseTypes %}
               {% if type.courseType !== 'Apprenticeship' %}
                <div class="courseGroup">
                  <div class="heading">
                     <h3 class="courseTypes mb-0">{{ type.courseType }}</h3>
                     {% if type.tagline %}
                     <p class="mb-6">{{type.tagline}}</p>
                     {% endif %}
                  </div>
                  {% for item in courses %}
                     {% if item.isActive and item.courseType !== 'Apprenticeship' and item.courseType === type.courseType %}
                        <div class="course-item flex flex-col md:flex-row rounded-lg shadow-lg overflow-hidden mb-6 {% if item.duration %}{% for item in item.duration %} {{item | join}}{% endfor %} {% endif %} {% if item.attendance %} {{ item.attendance | join }} {% endif %} {% if item.courseTopics %} {% for item in item.courseTopics %} {{ item.name | join }} {% endfor %}{% endif %} {% if item.title %}{{ item.title | join }}{% endif %} {% if item.courseType %}{{ item.courseType | join }}{% endif %} ">
                           <div class="w-full md:w-4/12">
                              <img class="h-full w-full object-cover" src="{{item.courseItemImage}}" alt="{{item.name}}">
                           </div>
                           <div class="bg-white flex flex-col justify-around px-4 md:ml-8 py-4 w-full md:w-8/12">
                              <div>
                                 <div class="flex mb-1">
                                 {% for item in item.courseTopics%}
                                    <a href="#" class="mr-2 p-2 bg-red-100 text-white font-bold rounded block font-heading text-sm leading-sm">{{item.name}}</a>
                                 {% endfor %}
                                 </div>

                                 <p class="text-xl font-semibold text-blue-200">
                                    <a href="{{item.slug.current}}">
                                       {{item.title}}
                                    </a>
                                 </p>

                              </div>
                              <div class="flex items-center w-full flex-wrap mt-4 md:mt-0">
                                 <div class="w-6/12 mb-2 md:mb-0">
                                    <p class=" flex items-center text-sm font-medium text-gray-900 leading-none">
                                    <span class="mr-2"><img style="height: 20px;" src="../../images/course-directory/Graduation-Cap-Icon.svg" alt="{{item.courseType}}" /></span>
                                       {{item.courseType}}
                                    </p>
                                 </div>
                                 <div class="w-6/12 mb-2 md:mb-0">
                                    <p class="flex items-center text-sm font-medium text-gray-900">
                                       <span class="mr-2"><img style="height: 20px;" src="../../images/course-directory/location.svg" alt="{{item.courseType}}" /></span>
                                       Classroom
                                    </p>
                                 </div>
                                 <div class="w-6/12">
                                    <p class="flex items-center text-sm font-medium text-gray-900">
                                       <span class="mr-2"><img style="height: 20px;" src="../../images/course-directory/calendar.svg" alt="{{item.courseType}}" /></span>
                                       {% if item.duration %}{% if item.duration.length < 2 %}  {{item.duration[0]}} {% else %} Multiple Options Available {% endif %}{% endif %}
                                    </p>
                                 </div>
                                 <div class="w-6/12">
                                    <p class=" flex items-center text-sm font-medium text-gray-900">
                                       <span class="mr-2"><img style="height: 20px;" src="../../images/course-directory/clock.svg" alt="{{item.courseType}}" /></span>
                                       {{item.attendance}}
                                    </p>
                                 </div>
                              </div>
                           </div>
                        </div>
                     {% endif %}
                  {% endfor %}
                  </div>
               {% endif %}
               {% endfor %}
            </div>
         </div>
      </div>
   </section>

      <section id="apprenticeships" class="relative overflow-hidden bg-blue-200">
         <object type="image/svg+xml" data="../images/patterns/3.svg" class="hidden md:block absolute pattern pattern-3"></object>
            <div class="inner relative z-10">
            <div class="mt-24 mb-32">
               {% include "components/apprenticeships/apprenticeships-component.njk" %}
            </div>
            </div>
         <object type="image/svg+xml" data="../images/patterns/4.svg" class="hidden md:block absolute bottom-0 pattern pattern-4"></object>
      </section>
   </main>
</div>