<div class="bg-grey-100 py-24 section-rounded p-4 lg:px-48 xl:px-64">
    <object aria-hidden="true" type="image/svg+xml" data="../../images/patterns/11.svg" class="hidden lg:block absolute pattern pattern-11"></object>
    <object aria-hidden="true" type="image/svg+xml" data="../../images/patterns/16.svg" class="hidden lg:block absolute pattern pattern-16"></object>
    
    <div class="max-w-3xl mx-auto">
        <div class="flex flex-col justify-center mx-auto">
            <div class="flex items-center gap-4">
                <div class="block h-1 w-12 bg-grey-1000 rounded"></div>
                <p class="mb-0 font-sm font-bold text-grey-1000 !important">Course Content</p>
            </div>
            <div>
                <h3>{{ course.title }}</h3>
            </div>
        </div>
        <div class="cursor-pointer flex flex-col items-center max-w-7xl mx-auto bg-white faq">
            {% for block in course.content %}
                <div class="py-4 border-grey-1000 border-1 px-6 w-full">
                    {% for key, value in block %}
                        <div class="flex justify-between items-center self-stretch faq-btn">
                            <div class="flex items-center gap-2">
                                <div>
                                    <svg xmlns="http://www.w3.org/2000/svg" width="10" height="16" viewBox="0 0 10 16" fill="none">
