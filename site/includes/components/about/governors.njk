
<section class="relative md:pt-20 pt-10 z-10 pb-4 md:pb-8 overflow-hidden">
    <object aria-hidden="true" type="image/svg+xml" data="../images/patterns/13.svg" class="hidden md:block absolute pattern pattern-3"></object>
    <div class="inner">
      <h2 class="text-center md:mb-16 mb-10">Meet our Governors!</h2>
      <div class="flex flex-wrap -mx-7">
        {% for member in governors %}
            <div class="sm:w-1/2 md:w-1/3 text-center px-7 md:mb-16 mb-14 team__member member-on-print">
                <img class="h-35 w-35 object-cover border-4 border-blue-100 rounded-50 mb-6 mx-auto" src="{{ member.image }}"/>
                <h3 class="mb-4">{{ member.name }}</h3>
                <span class="mb-4 text-md leading-sm block font-bold font-heading">{{ member.occupation }}</span>
                <p class="mb-4 text-sm leading-md">
                {{ member.bio }}
                </p>
                <div class="flex justify-center">
                {% if member.linkedin %}
                    <a href="{{ member.linkedin }}" class="p-2">
                    <span class="sr-only">Linkedin</span>
                    <img src="../images/icons/linkedin.svg" class="h-4"/>
                    </a>
                {% endif %}
                {% if member.twitter %}
                    <a href="{{ member.twitter }}" class="p-2">
                    <span class="sr-only">Twitter</span>
                    <img src="../images/icons/twitter.svg" class="h-4"/>
                    </a>
                {% endif %}
                {% if member.github %}
                    <a href="{{ member.github }}" class="p-2">
                    <span class="sr-only">Github</span>
                    <img src="../images/icons/github.svg" class="h-4"/>
                    </a>
                {% endif %}
                </div>
            </div>
        {% endfor %}
      </div>
    </div>
    <object aria-hidden="true" type="image/svg+xml" data="../images/patterns/14.svg" class="hidden md:block absolute pattern pattern-4"></object>
    <object aria-hidden="true" type="image/svg+xml" data="../images/patterns/15.svg" class="hidden md:block absolute pattern pattern-5"></object>
  </section>