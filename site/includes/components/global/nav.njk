{% if page.url === '/course-directory/get-hired-as-a-software-tester/' or page.url === '/course-directory/get-hired-as-a-ux-professional/' or page.url === '/course-directory/digital-project-manager/' or page.url === '/course-directory/essential-skills-for-your-tech-career/' or page.url === '/course-directory/ux-in-5-days/' or page.url === '/course-directory/problem-solving-with-javascript/' or page.url === '/course-directory/source-control-and-workflows-with-git-5-day/' or page.url === '/course-directory/practical-agile-in-5-days/'  %}
<div class="nav mb-4">
  <div>
    <button id="nav-toggle" class="burger relative block z-20">
      <span class="block bg-blue-100 rounded"></span>
      <span class="block bg-blue-100 rounded"></span>
      <span class="block bg-blue-100 rounded"></span>
    </button>
    <div class="mobile-nav-container opacity-0 fixed z-10 h-full flex bg-white">
      <nav id="mobile-nav" class="inner">
        <ul class="py-24 nav-links">
          {% for nav_link in navigation.main %}
          {% if nav_link.submenu %}
          <li class="px-4 text-center relative parent block py-2 font-bold font-heading text-black capitalize text-3xl leading-4xl">{{ nav_link.label | safe }}<i class="dropdown-trigger align-middle ml-2 fas fa-angle-down text-md leading-md" aria-hidden="true"></i>

              <ul class="submenu absolute shadow-secondary hidden">
                {% for link in nav_link.submenu%}
                  <li class="px-4 hover:bg-blue-200 hover:text-white">
                    <a href="{{link.url | safe}}" class="block py-2 font-bold font-heading text-black capitalize text-xl leading-xl">{{ link.label | safe }}
                    </a>
                  </li>
                {% endfor %}
              </ul>
          </li>
          {% else %}
            <li class="my-8 text-center">
              <a href="{{ nav_link.url | safe }}" class="block py-2 font-bold font-heading text-black capitalize text-3xl leading-4xl">{{ nav_link.label | safe }}
              </a>
            </li>
          {% endif %}
          {% endfor %}
